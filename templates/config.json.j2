{
"API_SERVER_LOC": "tcp://{{ inbox.listen.api }}",
"CRISPIN_SERVER_LOC": "tcp://{{ inbox.listen.crispin }}",
"WEBHOOK_SERVER_LOC": "tcp://{{ inbox.listen.webhook }}",

"MYSQL_DATABASE": "{{ inbox.database.name }}",
"MYSQL_USER": "{{ inbox.database.user }}",
"MYSQL_PASSWORD": "{{ inbox.database.password }}",
