"MYSQL_PORT": 3306,

"DB_POOL_SIZE": 25,

"CACHE_BASEDIR" : "{{ inbox.paths.lib }}/cache",
"LOGDIR": "{{ inbox.paths.log }}",
"LOGLEVEL": {{ inbox.loglevel }},

"GOOGLE_OAUTH_CLIENT_ID": "{{ inbox.google_oauth.client_id }}",
